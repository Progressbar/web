<div id='actions'>
  <ul>
    <% if ::Refinery::Mailinglists::Admin::SubscribersController.searchable? %>
      <li>
        <%= render '/refinery/admin/search', :url => refinery.mailinglists_admin_subscribers_path %>
      </li>
    <% end %>
    <li <%= "class='selected'" if params[:action] == "index" %>>
      <%= link_to t('.inbox'), refinery.mailinglists_admin_subscribers_path, :class => "email_icon" %>
    </li>
  </ul>
</div>
